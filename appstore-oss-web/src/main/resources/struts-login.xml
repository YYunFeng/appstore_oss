<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="login" namespace="/login" extends="appstore-default">
        <action name="*" method="{1}" class="com.jd.cms.web.action.login.LoginAction">
            <result name="success">/WEB-INF/vm/login/login.vm</result>
            <result name="init">/WEB-INF/vm/login/welcome.vm</result>
        </action>
    </package>
</struts>
