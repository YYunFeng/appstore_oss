<!--渠道下按应用查看安装次数-->
#set($layout="content.vm")
<script type="text/javascript">
    function macroAjaxSubmit(url) {
        var startTime = $("#d5221").val();
        var endTime = $("#d5222").val();
        var flag = $("#marketFlag").val();
        var cId = $("#cId").val();
        jQuery.ajax({
            type: 'post',
            url: url,
            data: 'startTime=' + startTime + "&endTime=" + endTime + "&flag=" + flag + "&channelId=" + cId,
            dataType: "html",
            success: function (msg) {
                jQuery("#marketStatic").html(msg);
            }
        });
    }
</script>
<div class="paging fr">
    <div class="paging-box_2">
        #set($pageModule = $domain.getTarget("/marketStatistics/statChannelByApps.action").addQueryData("pageSize",$!pageSize))
                        #showPage($pageModule $statChannelByApps)
    </div>
</div>
<div>
    #if($flag==0)
        离线应用-->
    #elseif($flag==1)
        在线应用-->
    #end
    <a href="javascript:void(0)" onclick="installTotal();"> 应用的安装总量</a>--><a href="javascript:void(0)"
                                                                             onclick="statByChannel();">
    按渠道查看</a>--><span style="font-weight:bolder;">按应用查看</span>
</div>
<div class="tb-box">
    <table class="tb" cellspacing="0" cellpadding="0">
        <tr class="tit-bg">
            <td>应用图标</td>
            <td>应用名称</td>
            <td>安装手机台数</td>
            <td>安装量</td>
            <td>操作</td>
        </tr>
        #foreach($marketings in $statChannelByApps)
            <tr>
                <td><img width="75" height="75"
                    #if($!marketings.zhuoWangMark == 0)
                         src="$imgserver.getTarget('')cover/$!marketings.logoUrl"
                    #else
                         src="$!marketings.logoUrl"
                    #end />
                </td>
                <td>$!marketings.appName</td>
                <td>$!marketings.phoneTimes</td>
                <td>$!marketings.installTimes</td>
                <td>
                    <a href="javascript:void(0);" onclick="appStatsChannelBySaler($!marketings.appId,$marketings.cId)">
                        按促销员查看
                    </a>
                </td>
            </tr>
        #end
    </table>
</div>
<div class="paging fr">
    <div class="paging-box_2">
        #set($pageModule = $domain.getTarget("/marketStatistics/statChannelByApps.action").addQueryData("pageSize",$!pageSize))
                        #showPage($pageModule $statChannelByApps)
    </div>
</div>
